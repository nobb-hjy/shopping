/* pages/category/index.wxss */
.cate{
  height: 100%;
}
.content{
  height: calc(100vh - 90rpx);
  display: flex;
}
.content .left-box,.content .right-box{
  height: 100%;
}
.content .left-box{
  width:30% ;
  text-align: center;
}
.content .left-box view{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
}
.content .right-box{
  flex: 1;
}
.content .right-box .item-box .item-title{
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
}
.content .right-box .item-box .line{
  color: #ccc; 
  padding: 20rpx;
}
.content .right-box .item-box .item-list{
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: flex-start;
}
.content .right-box .item-box .item-list navigator{
  width: 33.33%;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  font-size: 24rpx;
  margin-bottom: 20rpx;
}
.content .right-box .item-box .item-list navigator image{
  width: 70%;
}
.content .right-box .item-box .item-list navigator view{
  width: 100%;
  text-align: center;
}

.kong{
  width: 100%;
  height: 80rpx;
  text-align: center;
  color: #ccc;
  line-height: 80rpx;
  font-size: 24rpx;
}
.activeItem{
  color: var(--themeColor);
}